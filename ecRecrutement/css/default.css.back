.newsthousand {
	background: url('http://i.imgur.com/Q5arH.gif');
	color: inherit;
font-weight: normal;
	-webkit-animation:newsthousand 5s linear infinite;
   -moz-animation:newsthousand 5s linear infinite;
    -ms-animation:newsthousand 5s linear infinite;
     -o-animation:newsthousand 5s linear infinite;
        animation:newsthousand 5s linear infinite;
}


    @-moz-keyframes newsthousand {
        0% { color: #FF1919; }
        15% { color: #E019FF; }
        30% { color: #4F19FF; }
        45% { color: #19A7FF; }
        60% { color: #19FFDC; }
        75% { color: #65FF19; }
        90% { color: #FFE019; }
    }
    @-ms-keyframes newsthousand {
        0% { color: #FF1919; }
        15% { color: #E019FF; }
        30% { color: #4F19FF; }
        45% { color: #19A7FF; }
        60% { color: #19FFDC; }
        75% { color: #65FF19; }
        90% { color: #FFE019; }
    }
    @-webkit-keyframes newsthousand {
        0% { color: #FF1919; }
        15% { color: #E019FF; }
        30% { color: #4F19FF; }
        45% { color: #19A7FF; }
        60% { color: #19FFDC; }
        75% { color: #65FF19; }
        90% { color: #FFE019; }
    }
    @-o-animation newsthousand {
        0% { color: #FF1919; }
        15% { color: #E019FF; }
        30% { color: #4F19FF; }
        45% { color: #19A7FF; }
        60% { color: #19FFDC; }
        75% { color: #65FF19; }
        90% { color: #FFE019; }
    }

