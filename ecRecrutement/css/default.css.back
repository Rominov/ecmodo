[4. Main / .main]
*/

.main {
	padding-bottom: 2em;
	
}



/*------------------------------------------------------------------
[5. Extra / .extra]
*/

.extra {

	border-top: 1px solid #3498db;

}

.extra-inner {
	padding: 20px 0;
	
	font-size: 14px;
	color: white;
	
	background: #3498db;
}

.extra a {
	color: white;
}

.extra a:hover {
	color: white;
}

.extra h4 {
	margin-bottom: 1em;
	
	font-weight: 400;
}

.extra ul {
	padding: 0;
	margin: 0;
}

.extra li {
	margin-bottom: .6em;
	
	list-style: none;
}

.rounded-img {
	-webkit-transition-duration: 0.8s;
	-moz-transition-duration: 0.8s;
	-o-transition-duration: 0.8s;
	transition-duration: 0.8s;
	-webkit-transition-property: -webkit-transform;
	-moz-transition-property: -moz-transform;
	-o-transition-property: -o-transform;
	transition-property: transform;
	-webkit-animation: rounded-img 1s linear infinite;
	-moz-animation: rounded-img 1s linear infinite;
	-ms-animation: rounded-img 1s linear infinite;
	-o-animation: rounded-img 1s linear infinite;
	animation: rounded-img 1s linear infinite;
}

.rounded-img:hover {
	-webkit-transform:rotate(360deg);
	-moz-transform:rotate(360deg);
	-o-transform:rotate(360deg);
}

.txt-opacity {
	-webkit-transition-duration: 0.8s;
	-moz-transition-duration: 0.8s;
	-o-transition-duration: 0.8s;
	transition-duration: 0.8s;
	-webkit-transition-property: -webkit-transform;
	-moz-transition-property: -moz-transform;
	-o-transition-property: -o-transform;
	transition-property: transform;
	-webkit-animation: txt-opacity 1s linear infinite;
	-moz-animation: txt-opacity 1s linear infinite;
	-ms-animation: txt-opacity 1s linear infinite;
	-o-animation: txt-opacity 1s linear infinite;
	animation: txt-opacity 1s linear infinite;
}

.txt-opacity:hover {
	-webkit-transform:rotate(3deg);
	-moz-transform:rotate(3deg);
	-o-transform:rotate(3deg);
}


